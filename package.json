{
    "name": "@webzaytsev/yookassa-ts-sdk",
    "version": "3.0.5",
    "description": "TypeScript SDK for the YooKassa API with retries, idempotence helpers, rate limiting, proxy support, and Bun/Node compatibility.",
    "author": "WEBzaytsev <@WEBzaytsev>",
    "contributors": [
        "Dmitriy <@Mityayka1>",
        "Aleksey Aleksyuk <@awardix>"
    ],
    "maintainers": [
        "WEBzaytsev <@WEBzaytsev>"
    ],
    "keywords": [
        "yookassa",
        "yookassa-sdk",
        "yookassa-ts",
        "yookassa-api",
        "payment",
        "refunds",
        "receipts",
        "payments",
        "sberpay",
        "T-pay",
        "yandexpay",
        "SBP",
        "api",
        "typescript",
        "javascript",
        "nodejs",
        "node",
        "bun",
        "sdk",
        "russia",
        "russian-payments",
        "e-commerce",
        "ecommerce",
        "online-payments",
        "checkout",
        "payment-processing",
        "proxy"
    ],
    "private": false,
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "README.ru.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WEBzaytsev/yookassa-ts-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/WEBzaytsev/yookassa-ts-sdk/issues"
    },
    "homepage": "https://github.com/WEBzaytsev/yookassa-ts-sdk#readme",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "bun run src/tests/man-tests.ts",
        "lint": "biome check src/",
        "lint:fix": "biome check --write src/",
        "format": "biome format --write src/",
        "knip": "knip",
        "check": "npm run knip && npm run lint && tsc --noEmit"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "axios-logger": "^2.8.1",
        "axios-rate-limit": "^1.4.0",
        "https-proxy-agent": "^7.0.6"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.8",
        "@types/node": "^22.0.2",
        "knip": "^5.73.4",
        "typescript": "^5.5.4"
    }
}
